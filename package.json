{
  "name": "sparkvibe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node backend/server.js",
    "start-frontend": "cd frontend && npm run start",
    "dev-backend": "cd backend && nodemon server.js",
    "dev-frontend": "cd frontend && npm run dev",
    "install-backend": "npm install --prefix backend",
    "install-frontend": "npm install --prefix frontend",
    "install-all": "npm run install-backend && npm run install-frontend",
    "build-frontend": "cd frontend && npm install && npm run build && npm run export"
  },
  "dependencies": {
    "@fastify/static": "^6.12.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/jwt": "^7.2.0",
    "@fastify/multipart": "^8.0.0",
    "@huggingface/inference": "^2.8.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "dotenv": "^16.3.0",
    "fastify": "^4.24.0",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.0.0",
    "openai": "^4.104.0",
    "redis": "^4.6.0",
    "web-push": "^3.6.0",
    "concurrently": "^7.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.5.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}